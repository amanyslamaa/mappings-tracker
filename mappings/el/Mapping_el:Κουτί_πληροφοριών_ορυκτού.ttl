### Main TriplesMap
####################
<http://el.dbpedia.org/resource/Mapping_el:Κουτί_πληροφοριών_ορυκτού>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/0> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:Mineral ;
        rr:template  "http://el.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Κουτί_πληροφοριών_ορυκτού" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://el.wikipedia.org/wiki/{wikititle}" .


