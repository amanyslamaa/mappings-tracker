### Main TriplesMap
####################
<http://vi.dbpedia.org/resource/Mapping_vi:Thông_tin_nhạc_sĩ>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/7> ,
			       <SimplePropertyMapping/18> ,
			       <SimplePropertyMapping/13> ,
			       <SimplePropertyMapping/8> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/16> ,
			       <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/11> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/14> ,
			       <SimplePropertyMapping/9> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/12> ,
			       <SimplePropertyMapping/10> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/17> ,
			       <SimplePropertyMapping/15> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:MusicalArtist ;
        rr:template  "http://vi.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Thông_tin_nhạc_sĩ" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://vi.wikipedia.org/wiki/{wikititle}" .


