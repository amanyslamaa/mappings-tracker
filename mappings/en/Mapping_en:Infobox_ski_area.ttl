### Main TriplesMap
####################
<http://en.dbpedia.org/resource/Mapping_en:Infobox_ski_area>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <LatitudeMapping/1> ,
			       <LongitudeMapping/1> ,
			       <LongitudeMapping/0> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/1> ,
			       <LatitudeMapping/0> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/2> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:SkiArea ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Infobox_ski_area" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://en.wikipedia.org/wiki/{wikititle}" .


